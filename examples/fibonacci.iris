// fibonacci.iris -- recursive Fibonacci
//
// Run: cargo run -- --emit eval examples/fibonacci.iris

def fib(n: i64) -> i64 {
    if n <= 1 {
        n
    } else {
        fib(n - 1) + fib(n - 2)
    }
}

def main() -> i64 {
    val result = fib(8)
    print(concat("fib(8) = ", to_str(result)));
    result
}
